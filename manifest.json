{
  "manifest_version": 2,

  "name": "Calendar View Homepage",
  "description": "This extension allows your calendar to be displayed for the week",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click to view calendar!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}